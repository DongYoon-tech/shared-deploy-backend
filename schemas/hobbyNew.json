{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "activity": "play soccer",
            "meet_address": "Sab Francisco, CA",
            "user_username": "Test",
            "lat": 37.76271616903525,
            "lng": -122.4281591016166
        }
    ],
    "required": [
        "activity",
        "meet_address",
        "lat",
        "lng"
    ],
    "properties": {
        "activity": {
            "$id": "#/properties/activity",
            "type": "string",
            "title": "The activity schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "play soccer"
            ]
        },
        "meet_address": {
            "$id": "#/properties/meet_address",
            "type": "string",
            "title": "The meet_address schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Sab Francisco, CA"
            ]
        },
        "user_username": {
            "$id": "#/properties/user_username",
            "type": "string",
            "title": "The user_username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Test"
            ]
        },
        "lat": {
            "$id": "#/properties/lat",
            "type": "number",
            "title": "The lat schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0.0,
            "examples": [
                37.76271616903525
            ]
        },
        "lng": {
            "$id": "#/properties/lng",
            "type": "number",
            "title": "The lng schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0.0,
            "examples": [
                -122.4281591016166
            ]
        }
    },
    "additionalProperties": true
}